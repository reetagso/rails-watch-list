<%# <h1>Add new bookmark to <%= @bookmark.list</h1> %>

<div class="container">
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
</div>

